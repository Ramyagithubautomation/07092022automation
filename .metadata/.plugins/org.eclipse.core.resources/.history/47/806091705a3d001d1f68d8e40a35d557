package FirstPackage;

public class accessmodifiersclass {
	
	public int publicvar=10;
	protected int protectvar=20;
	int defaultvar=30;
	private final int privatevar=40;

	public static void main(String[] args) {
		
		final int localvar=100;
		accessmodifiersclass modify=new accessmodifiersclass();
		
		System.out.println(modify.publicvar);
        modify.publicmethod();
        System.out.println(modify.protectvar);
        System.out.println(modify.defaultvar);
        System.out.println(modify.privatevar);
        //modify.privatevar=50;

	}
	
	
	public void publicmethod()
	{
		System.out.println("publicmethod");
	}

}
