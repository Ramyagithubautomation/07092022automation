package FirstPackage;

public class Flowcontrolstatements {

	public static void main(String[] args) {

		
		System.out.println(tax(20000));
		
		
		
	}
	
	
	public static int tax(int salary)
	{
		int taxpercentage=0;
		
		if(salary>0 && salary<10000)
		{
			taxpercentage=5;
			
		}
		
		/*else
		{
			taxpercentage=10;
		}*/
		
		return taxpercentage;
	}
	

}
